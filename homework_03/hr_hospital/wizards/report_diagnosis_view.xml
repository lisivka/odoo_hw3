<odoo>
    <data>
        <record id="view_report_diagnosis_wizard" model="ir.ui.view">
            <field name="name">report.diagnosis.wizard.form</field>
            <field name="model">report.diagnosis.wizard</field>
            <field name="arch" type="xml">
                <form string="Generate Diagnosis Report">
                    <group>
                        <field name="date_from"/>
                        <field name="date_to"/>
                    </group>
                    <footer>
                        <button string="Generate Report" type="object" name="action_generate_report" class="btn-primary"/>
                        <button string="Cancel" class="btn-secondary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>
